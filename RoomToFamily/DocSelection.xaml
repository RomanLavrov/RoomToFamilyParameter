<UserControl
    x:Class="RoomToFamily.DocSelection"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:RoomToFamily"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="300"
    d:DesignWidth="480"
    mc:Ignorable="d">
    <Grid>
        <Grid
            Name="Grid"
            Width="445"
            Height="215"
            Margin="25,45,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top">
            <Label
                Width="280"
                Margin="-15,-35,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Content="Please select project to search rooms:"
                FontSize="16" />
        </Grid>
        <Button
            Width="85"
            Margin="200,270,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Click="Button_Click"
            Content="OK" />
    </Grid>
</UserControl>
